}
@article{Kwon1999,
	abstract = {This paper treats modeling and control for the burnthrough point$\backslash$nin industrial sintering processes. First, a simple state-space model for$\backslash$nevent-time dynamics is derived for a complicated system by introducing$\backslash$nevents into a continuous variable system. For the control of the$\backslash$nsintering process, a two-stage control policy is used. First, optimal$\backslash$ninterevent time is obtained from an output-constrained receding horizon$\backslash$ncontrol with a least-square prediction algorithm. Then the average$\backslash$nstrand speeds for the interevent times are obtained and applied as a$\backslash$ncommand signal to a motor control system. It is proved that the proposed$\backslash$noutput-constrained receding horizon control law with a short horizon$\backslash$nstabilizes the closed-loop system and the steady-state error for a$\backslash$nset-point becomes zero. The real-time experiments are carried out in a$\backslash$nPOSCO (Pohang Steel Company, Korea) sintering plant and satisfactory$\backslash$nresults are presented in this paper. Also, computer simulations are$\backslash$ncarried out and compared with the real-time experiments},
	author = {Kwon, Wook Hyun and Kim, Yong Ho and Lee, Sang Jeong and Paek, Ki Nam},
	doi = {10.1109/87.736747},
	issn = {10636536},
	journal = {IEEE Transactions on Control Systems Technology},
	keywords = {Least-square prediction algorithm,Modeling,Receding horizon control,Sintering process,Steel company},
	title = {{Event-Based modeling and control for the burnthrough point in sintering processes}},
	year = {1999},
	volume={7},
	pages = {31--41}
}
